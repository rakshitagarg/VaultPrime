  <body>
    <%- include("../utils/header")%> <%- include("../utils/side")%>

    <main id="main" class="main">
      <div class="pagetitle">
        <h1>Data Tables</h1>
        <nav>
          <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/dashboard">Home</a></li>
            <li class="breadcrumb-item">Services</li>
          </ol>
        </nav>
      </div>
      <!-- End Page Title -->

      <section class="section">
        <div class="row">
          <div class="col-lg-12">
            <div class="card" style="overflow-x: scroll">
              <div class="card-body">
                <h5 class="card-title">Datatables</h5>
                <!-- Table with stripped rows -->
                <table class="table datatable">
                  <thead>
                    <tr>
                      <th scope="col">S No.</th>
                      <th scope="col">User Name</th>
                      <th scope="col">Email</th>
                      <th scope="col">Mobile</th>
                    </tr>
                  </thead>

                  <tbody>
                    <div>
                      <%let i=1; data.forEach((val)=>{ %>
                      <tr>
                        <th scope="row"><%=i%></th>
                        <td><%=val.name%></td>
                        <td><%=val.email%></td>
                        <td><%=val.mobile%></td>
                        <td>
                            <button><a href="update/<%=val.email %>">Update</a></button>
                          </td>
                            <td>
                          <form action="delete" method="post">
                            <input type="hidden" value="<%= val.email %>" name="email"/>
                            <input type="submit" value="Delete" name="del" />
                          </td>
                        </form>
                          <td>
                            <form action="block_user" method="post">
                              <input type="hidden" value="<%= val.email %>" name="email"/>
                              <input type="submit" value="Block" name="but" />
                                <input type="submit" value="Unblock" name="but" />
                            </td>
                          </td>
                        </td>
                      
                      </form>
                      </tr>
                      <% i=i+1; })%>
                    </div>
                  </tbody>
                </table>
                <!-- End Table with stripped rows -->
              </div>
            </div>
          </div>
        </div>
      </section>
    </main>
    <!-- End #main -->

    <%- include("../utils/footer")%>

    <a
      href="#"
      class="back-to-top d-flex align-items-center justify-content-center"
      ><i class="bi bi-arrow-up-short"></i
    ></a>
    <script>
      function confirmDelete() {
        // Display a confirmation dialog
        var result = confirm("Are you sure you want to delete this user?");

        // If the user clicks OK, the form will be submitted. Otherwise, it won't.
        return result;
      }
    </script>
